{
  "name": "garbage-recycle-scale",
  "private": true,
  "version": "0.0.4",
  "main": "./.vite/build/index.cjs",
  "type": "module",
  "scripts": {
    "dev": "concurrently -k \"vite\" \"wait-on tcp:5173 && electron-forge start\"",
    "dev:instance1": "set INSTANCE_ID=1 && set ALLOW_MULTIPLE_INSTANCES=true && concurrently -k \"vite\" \"wait-on tcp:5173 && electron-forge start\"",
    "dev:instance2": "set INSTANCE_ID=2 && set ALLOW_MULTIPLE_INSTANCES=true && set SKIP_REBUILD=true && concurrently -k \"vite\" \"wait-on tcp:5173 && electron-forge start\"",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "lint": "eslint . --ext .ts,.tsx",
    "rebuild": "electron-rebuild -f -w better-sqlite3 usb serialport"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.6.0",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.6.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-vite": "^7.6.0",
    "@electron/rebuild": "^4.0.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.0.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/usb": "^2.0.5",
    "@types/ws": "^8.5.13",
    "@vitejs/plugin-react": "^5.0.3",
    "baseline-browser-mapping": "^2.8.30",
    "concurrently": "^9.0.0",
    "electron": "^31.7.7",
    "eslint": "^9.12.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.0",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.29.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/pdfkit": "^0.17.3",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^12.4.6",
    "pdfkit": "^0.17.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "serialport": "^12.0.0",
    "usb": "^2.16.0",
    "ws": "^8.18.0",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9",
    "utf-8-validate": "^6.0.5"
  }
}
